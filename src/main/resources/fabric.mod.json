{
  "schemaVersion": 1,
  "id": "tnze-s-ime-mod",
  "version": "${version}",
  "name": "Tnze's IME Mod",
  "description": "Tnze's IME support mod for Minecraft",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "icon": "assets/tnze-s-ime-mod/icon.png",
  "environment": "client",
  "accessWidener": "tnze-s-ime-mod.accesswidener",
  "entrypoints": {
    "client": [
      "tech.tnze.client.IMEClient"
    ],
    "main": [
      "tech.tnze.IME"
    ],
    "modmenu": [
      "tech.tnze.modmenu.ModMenuApiImpl"
    ]
  },
  "mixins": [
    "tnze-s-ime-mod.mixins.json",
    {
      "config": "tnze-s-ime-mod.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/client/gui/components/EditBox": ["tech/tnze/client/ACPSinkRegister"]
    }
  }
}
